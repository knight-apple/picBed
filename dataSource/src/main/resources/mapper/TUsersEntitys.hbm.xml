<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.knightapple.dataSource.entity.TUsersEntitys" table="users" schema="picBed">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="email">
            <column name="email" sql-type="varchar(255)"/>
        </property>
        <property name="userName">
            <column name="userName" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="type">
            <column name="type" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>